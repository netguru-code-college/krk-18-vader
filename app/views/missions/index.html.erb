<p id="notice"><%= notice %></p>

<h1>Missions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <div class="container">
    <% @missions.each do |mission| %>
      <ul class="list-group">
        <li class="list-group-item list-group-item-light"><%= link_to mission.name, mission %></li>
      </ul>
    <% end %>
</div>

<br>

<%= link_to 'New Mission', new_mission_path %>
